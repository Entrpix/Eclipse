{
  "name": "eclipse",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && npx tsc --project tsconfig.server.json",
    "preview": "vite preview",
    "server": "node server.cjs",
    "start": "concurrently \"npm:server\" \"npm:dev\"",
    "bstart": "npm run build && npm run start",
    "lint": "eslint '*/**/*.{js,ts,tsx,jsx}' --quiet --fix"
  },
  "dependencies": {
    "@tomphttp/bare-server-node": "^2.0.3",
    "preact": "^10.19.6",
    "wisp-server-node": "^1.0.4"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.8.2",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "eslint": "^9.1.1",
    "eslint-config-preact": "^1.3.0",
    "eslint-plugin-react": "^7.34.1",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.0"
  }
}
