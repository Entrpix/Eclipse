{
  "name": "eclipse",
  "description": "Modern Webproxy",
  "author": "Entrpix",
  "version": "0.1.0",
  "license": "GPL-3.0-only",
  "repository": "https://github.com/entrpix/eclipse",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && npx tsc --project tsconfig.server.json",
    "preview": "vite preview",
    "server": "node server.cjs",
    "start": "concurrently \"npm:server\" \"npm:dev\"",
    "bstart": "npm run build && npm run start",
    "lint": "eslint '*/**/*.{js,ts,tsx,jsx}' --quiet --fix",
    "format": "prettier --write .",
    "check-format": "prettier --check ."
  },
  "dependencies": {
    "@mercuryworkshop/bare-mux": "^1.0.8",
    "@mercuryworkshop/epoxy-transport": "^1.1.0",
    "@mercuryworkshop/libcurl-transport": "^1.3.1",
    "@titaniumnetwork-dev/ultraviolet": "^3.0.2",
    "@tomphttp/bare-server-node": "^2.0.3",
    "fastify": "^4.26.2",
    "preact": "^10.19.6",
    "preact-iso": "^2.6.2",
    "rammerhead": "https://github.com/NebulaServices/rammerhead/releases/download/rammerhead-1.2.41-nebula.8/rammerhead-1.2.41-nebula.7.tgz",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "wisp-server-node": "^1.0.4"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.8.2",
    "@types/node": "^20.12.7",
    "@types/react-helmet": "^6.1.11",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "eslint": "^9.1.1",
    "eslint-config-preact": "^1.3.0",
    "eslint-plugin-react": "^7.34.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.0"
  }
}
